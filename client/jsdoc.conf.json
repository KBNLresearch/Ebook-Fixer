{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown", 
    "better-docs/component", 
    "node_modules/better-docs/category"
  ],
  "templates": {
    "better-docs": {
      "name": "Fixing E-Books Client Documentation"
    }
  },
  "opts": {
    "destination": "../client-docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}